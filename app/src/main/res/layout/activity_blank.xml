<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.kwsoft.kehuhua.adcustom.BlankActivity">

    <RelativeLayout
        android:id="@+id/personal_center_title"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:background="#393F4D">
        <ImageView
            android:id="@+id/personal_center_back_home"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:layout_centerVertical="true"
            android:src="@drawable/back"
            android:layout_marginTop="20dp"
            android:layout_alignParentBottom="true"/>
    </RelativeLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/personal_center_title"
        android:src="@drawable/authority"
        android:scaleX="0.6"
        android:scaleY="0.6"
        />
</RelativeLayout>
